<block>
  <name>OFDM Divide</name>
  <key>radar_ofdm_divide_vcvc</key>
  <category>RADAR</category>
  <import>import radar</import>
  <make>radar.ofdm_divide_vcvc($vlen_in, $vlen_out, $discarded_carriers, $len_key)</make>
  <param>
    <name>Vector length input</name>
    <key>vlen_in</key>
    <type>int</type>
  </param>
  <param>
    <name>Vector length output</name>
    <key>vlen_out</key>
    <type>int</type>
  </param>
  <param>
    <name>Discarded carriers</name>
    <key>discarded_carriers</key>
    <type>int_vector</type>
  </param>
  <param>
    <name>Packet length key</name>
    <key>len_key</key>
    <value>"packet_len"</value>
    <type>string</type>
  </param>
  <sink>
    <name>IQ in</name>
    <type>complex</type>
    <vlen>$vlen_in</vlen>
    <nports>2</nports>
  </sink>
  <source>
    <name>IQ out</name>
    <type>complex</type>
    <vlen>$vlen_out</vlen>
  </source>
</block>
