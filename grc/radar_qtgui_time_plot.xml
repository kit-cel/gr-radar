<block>
  <name>QT GUI Time Plot</name>
  <key>radar_qtgui_time_plot</key>
  <category>RADAR</category>
  <import>import radar</import>
  <make>radar.qtgui_time_plot($interval, $label_y, $axis_y, $range_time, $label)</make>
  <param>
    <name>Update interval [ms]</name>
    <key>interval</key>
    <type>int</type>
  </param>
  <param>
    <name>Label y</name>
    <key>label_y</key>
    <type>string</type>
  </param>
  <param>
    <name>Axis y</name>
    <key>axis_y</key>
    <type>real_vector</type>
  </param>
  <param>
    <name>Range time [s]</name>
    <key>range_time</key>
    <type>float</type>
  </param>
  <param>
    <name>Label title</name>
    <key>label</key>
    <type>string</type>
  </param>
  <sink>
    <name>Msg in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
</block>
