<block>
  <name>Transpose Matrix</name>
  <key>radar_transpose_matrix_vcvc</key>
  <category>RADAR</category>
  <import>import radar</import>
  <make>radar.transpose_matrix_vcvc($vlen_in, $vlen_out, $len_key)</make>
  <param>
    <name>Vector length input</name>
    <key>vlen_in</key>
    <type>int</type>
  </param>
  <param>
    <name>Vector length output</name>
    <key>vlen_out</key>
    <type>int</type>
  </param>
  <param>
    <name>Packet length key</name>
    <key>len_key</key>
    <value>"packet_len"</value>
    <type>string</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
    <vlen>$vlen_in</vlen>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
    <vlen>$vlen_out</vlen>
  </source>
</block>
