<block>
  <name>OFDM Cyclic Prefix Remover</name>
  <key>radar_ofdm_cyclic_prefix_remover_cvc</key>
  <category>RADAR</category>
  <import>import radar</import>
  <make>radar.ofdm_cyclic_prefix_remover_cvc($fft_len, $cp_len, $len_key)</make>
  <param>
    <name>FFT length</name>
    <key>fft_len</key>
    <type>int</type>
  </param>
  <param>
    <name>CP length</name>
    <key>cp_len</key>
    <type>int</type>
  </param>
  <param>
    <name>Packet length key</name>
    <key>len_key</key>
    <value>"packet_len"</value>
    <type>string</type>
  </param>
  <sink>
    <name>IQ in</name>
    <type>complex</type>
  </sink>
  <source>
    <name>IQ out</name>
    <type>complex</type>
    <vlen>$fft_len</vlen>
  </source>
</block>
